<Rock>:
    # --outline
    canvas.before:
        Color:
            rgb: 1, 1, 1
        Rectangle:
            size: self.size
            pos: self.pos
            texture: self.texture
        Color:
            rgb: (1, .1, .1, 1)
        Line:
            width: 1
            rectangle: self.x, self.y, self.width, self.height
