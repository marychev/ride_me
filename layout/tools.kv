#:kivy 1.11.1


<Tools>:
    size_hint: 1, .2

    Image:

        canvas:
            Clear
            Color:
                rgba: .8, .8, .8, 1
            Ellipse:
                pos: self.pos
                size: self.size

        canvas.after:
            Color:
                rgba: .8, .3, .4, 1

            Ellipse:
                pos: self.x, self.y
                size: self.size

            Color:
                rgba: 0, .5, 0, 1
            Rectangle:
                pos: 20, 30
                size: 40, 40

    ImageOutline:


